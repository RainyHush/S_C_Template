      },
			{
				"rule_set": "geosite-category-ads-all",
				"server": "block"
      },
			{
				"outbound": "any",
				"server": "remoteDNS",
				"disable_cache": true
      },
			{
				"rule_set": "geosite-cn",
				"server": "localDNS"
      },
			{
				"clash_mode": "direct",
				"server": "localDNS"
      },
			{
				"clash_mode": "global",
				"server": "fakeip"
      },
			{
				"inbound": "tun-in",
				"query_type": [
          "A",
          "AAAA"
        ],
				"server": "fakeip",
				"rewrite_ttl": 1
      },
			{
				"rule_set": "geosite-geolocation-!cn",
				"server": "remoteDNS"
      }
    ],
		"final": "remoteDNS",
		"strategy": "prefer_ipv4",
		"reverse_mapping": true,
		"independent_cache": true
	},
	"inbounds": [
		{
			"tag": "tun-in",
			"type": "tun",
			"inet4_address": "172.19.0.0/30",
			"inet6_address": "fdfe:dcba:9876::0/126",
			"mtu": 9000,
