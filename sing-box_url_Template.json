  "dns": {
    "fakeip": {
      "enabled": true,
      "inet4_range": "198.18.0.0/15",
      "inet6_range": "fc00::/18"
    },
    "servers": [
      {
        "tag": "Proxy_dns",
        "address": "https://1.1.1.1/dns-query",
        "address_strategy": "prefer_ipv4",
        "detour": "全局"
      },
      {
        "tag": "local_dns",
        "address": "https://223.5.5.5/dns-query",
        "address_strategy": "prefer_ipv4",
        "detour": "direct"
      },
      {
        "tag": "fakeip",
        "address": "fakeip"
      },
      {
        "tag": "block",
        "address": "rcode://success"
      }
    ],
    "rules": [
      {
        "rule_set": "geosite-category-ads-all",
        "server": "block"
      },
      {
        "outbound": "any",
        "server": "local_dns",
        "disable_cache": true
      },
      {
        "rule_set": "geosite-cn",
        "server": "local_dns"
      },
      {
        "clash_mode": "direct",
        "server": "local_dns"
      },
      {
        "clash_mode": "global",
        "server": "Proxy_dns"
      },
      {
        "inbound": "tun",
        "query_type": [
          "A",
          "AAAA"
        ],
        "server": "Proxy_dns",
        "rewrite_ttl": 1
      },
      {
        "rule_set": "geosite-geolocation-!cn",
        "server": "Proxy_dns"
      }
    ],
    "final": "Proxy_dns",
    "strategy": "prefer_ipv4",
    "disable_cache": false,
    "disable_expire": false,
    "independent_cache": true
  },
  "inbounds": [
    {
      "tag": "tun",
      "type": "tun",
      "inet4_address": "172.19.0.0/30",
      "inet6_address": "fdfe:dcba:9876::0/126",
      "mtu": 9000,
      "stack": "mixed",
      "auto_route": true,
      "strict_route": true,
      "sniff": true,
      "platform": {
