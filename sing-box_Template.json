      },
      {
        "rule_set": "geoip-cn",
        "outbound": "China"
      },
      {
        "rule_set": "geosite-cn",
        "outbound": "China"
      },
      {
        "rule_set": "geosite-geolocation-!cn",
        "outbound": "Proxy"
      }
    ],
    "final": "Proxy",
    "rule_set": [
      {
        "tag": "geosite-private",
